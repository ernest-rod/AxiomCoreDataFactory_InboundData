{
	"name": "Colors_CSV_File",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage_Inbound_Data",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"BlobContainer": {
				"type": "string",
				"defaultValue": "inbounddatafiles"
			},
			"Directory": {
				"type": "string",
				"defaultValue": "PU-WWI-REG"
			}
		},
		"folder": {
			"name": "Axiom Datasets/Test Area/WideWorldImporters"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "Colors.csv",
				"folderPath": {
					"value": "@dataset().Directory",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().BlobContainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"nullValue": "NULL",
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "ColorID",
				"type": "String"
			},
			{
				"name": "ColorName",
				"type": "String"
			},
			{
				"name": "LastEditedBy",
				"type": "String"
			},
			{
				"name": "ValidFrom",
				"type": "String"
			},
			{
				"name": "ValidTo",
				"type": "String"
			}
		]
	}
}